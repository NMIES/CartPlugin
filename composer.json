{
    "name": "cart-plugin/cart-plugin",
    "description": "Changes the color and the text of the Add to cart button on click",
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "shopware/core": "6.5.*"
    },
    "extra": {
        "shopware-plugin-class": "CartPlugin\\CartPlugin",
        "label": {
            "de-DE": "CartPlugin",
            "en-GB": "CartPlugin"
        }
    },
    "autoload": {
        "psr-4": {
            "CartPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CartPlugin\\Tests\\": "tests/"
        }
    }
}
